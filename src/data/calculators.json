[
  {
    "id": "resistor-calculator",
    "title": "Resistor Calculator",
    "description": "Calculate resistance and power using Ohm's Law",
    "category": "Electronics",
    "inputs": [
      {
        "id": "voltage",
        "label": "Voltage",
        "symbol": "V",
        "unit": "Volts (V)",
        "type": "number",
        "required": true,
        "min": 0,
        "placeholder": "Enter voltage"
      },
      {
        "id": "current",
        "label": "Current",
        "symbol": "I",
        "unit": "Amperes (A)",
        "type": "number",
        "required": true,
        "min": 0,
        "placeholder": "Enter current"
      }
    ],
    "outputs": [
      {
        "id": "resistance",
        "label": "Resistance",
        "symbol": "R",
        "unit": "Ohms (Ω)",
        "formula": "voltage / current"
      },
      {
        "id": "power",
        "label": "Power",
        "symbol": "P",
        "unit": "Watts (W)",
        "formula": "voltage * current"
      }
