[
  {
    "id": "resistor-calculator",
    "title": "Resistor Calculator",
    "description": "Calculate resistance and power using Ohm's Law",
    "category": "Electronics",
    "inputs": [
      {
        "id": "voltage",
        "label": "Voltage",
        "symbol": "V",
        "unit": "Volts (V)",
        "type": "number",
        "required": true,
        "min": 0,
        "placeholder": "Enter voltage"
      },
      {
        "id": "current",
        "label": "Current",
        "symbol": "I",
        "unit": "Amperes (A)",
        "type": "number",
        "required": true,
        "min": 0,
        "placeholder": "Enter current"
      }
    ],
    "outputs": [
      {
        "id": "resistance",
        "label": "Resistance",
        "symbol": "R",
        "unit": "Ohms (Ω)",
        "formula": "voltage / current"
      },
      {
        "id": "power",
        "label": "Power",
        "symbol": "P",
        "unit": "Watts (W)",
        "formula": "voltage * current"
      }
    ]
  },
  {
    "id": "length-converter",
    "title": "Length Converter",
    "description": "Convert between inches and centimeters",
    "category": "Unit Conversion",
    "inputs": [
      {
        "id": "inches",
        "label": "Length in Inches",
        "symbol": "in",
        "unit": "Inches (in)",
        "type": "number",
        "required": false,
        "min": 0,
        "placeholder": "Enter length in inches"
      }
    ],
    "outputs": [
      {
        "id": "centimeters",
        "label": "Length in Centimeters",
        "symbol": "cm",
        "unit": "Centimeters (cm)",
        "formula": "inches * 2.54"
      },
      {
        "id": "feet",
        "label": "Length in Feet",
        "symbol": "ft",
        "unit": "Feet (ft)",
        "formula": "inches / 12"
      }
    ]
  },
  {
    "id": "bmi-calculator",
    "title": "BMI Calculator",
    "description": "Calculate Body Mass Index",
    "category": "Health",
    "inputs": [
      {
        "id": "weight",
        "label": "Weight",
        "symbol": "W",
        "unit": "Kilograms (kg)",
        "type": "number",
        "required": true,
        "min": 1,
        "placeholder": "Enter weight in kg"
      },
      {
        "id": "height",
        "label": "Height",
        "symbol": "H",
        "unit": "Meters (m)",
        "type": "number",
        "required": true,
        "min": 0.5,
        "max": 3,
        "placeholder": "Enter height in meters"
      }
    ],
    "outputs": [
      {
        "id": "bmi",
        "label": "Body Mass Index",
        "symbol": "BMI",
        "unit": "kg/m²",
        "formula": "weight / (height * height)"
      }
    ]
  }
]