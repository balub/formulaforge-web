[
  {
    "id": "resistor-calculator",
    "title": "Resistor Calculator",
    "description": "Calculate resistance and power using Ohm's Law",
    "inputs": [
      {
        "id": "voltage",
        "label": "Voltage",
        "symbol": "V",
        "unit": "Volts (V)",
        "type": "number",
        "required": true,
        "min": 0,
        "placeholder": "Enter voltage"
      },
      {
        "id": "current",
        "label": "Current",
        "symbol": "I",
        "unit": "Amperes (A)",
        "type": "number",
        "required": true,
        "min": 0,
        "placeholder": "Enter current"
      }
    ],
    "outputs": [
      {
        "id": "resistance",
        "label": "Resistance",
        "symbol": "R",
        "unit": "Ohms (Ω)",
        "formula": "voltage / current",
        "formula_display": "R = \\frac{V}{I}"
      },
      {
        "id": "power",
        "label": "Power",
        "symbol": "P",
        "unit": "Watts (W)",
        "formula": "voltage * current",
        "formula_display": "P = V \\cdot I"
      }
    ]
  },
  {
    "id": "led-series-resistor-calculator",
    "title": "LED Series Resistor Calculator",
    "description": "Calculate the required series resistor for LED circuits using Ohm's Law to ensure proper current limiting",
    "inputs": [
      {
        "id": "supply_voltage",
        "label": "Supply Voltage",
        "symbol": "Vs",
        "unit": "Volts (V)",
        "type": "number",
        "required": true,
        "min": 0,
        "max": 50,
        "step": 0.1,
        "placeholder": "12"
      },
      {
        "id": "voltage_drop",
        "label": "Voltage Drop",
        "symbol": "Vd",
        "unit": "Volts (V)",
        "type": "number",
        "required": true,
        "min": 0,
        "max": 10,
        "step": 0.1,
        "placeholder": "2"
      },
      {
        "id": "forward_current",
        "label": "Forward Current",
        "symbol": "If",
        "unit": "Amperes (A)",
        "type": "number",
        "required": true,
        "min": 0.001,
        "max": 5,
        "step": 0.001,
        "placeholder": "0.02"
      }
    ],
    "outputs": [
      {
        "id": "resistance",
        "label": "Resistance",
        "symbol": "R",
        "unit": "Ohms (Ω)",
        "formula": "(supply_voltage - voltage_drop) / forward_current",
        "formula_display": "R = \\frac{V_s - V_d}{I_f}"
      },
      {
        "id": "power_dissipation",
        "label": "Power Dissipation",
        "symbol": "P",
        "unit": "Watts (W)",
        "formula": "(supply_voltage - voltage_drop) * forward_current",
        "formula_display": "P = (V_s - V_d) \\cdot I_f"
      }
    ],
    "notes": [
      "Don't end with comma (,)",
      "Ensure the resistor's power rating exceeds the calculated power dissipation",
      "Use standard resistor values closest to the calculated resistance",
      "Typical LED forward voltages: Red ~2V, Green/Blue ~3.3V, White ~3.3V"
    ]
  },
  {
    "id": "ohms-law",
    "title": "Ohm's Law",
    "description": "Ohm's Law Yo",
    "inputs": [
      {
        "id": "input_1758792782858",
        "label": "Current",
        "symbol": "I",
        "unit": "Amperes (A)",
        "type": "number",
        "required": true,
        "placeholder": "Enter value"
      },
      {
        "id": "input_1758792809110",
        "label": "Resistance",
        "symbol": "R",
        "unit": "Ohms (Ω)",
        "type": "number",
        "required": true,
        "placeholder": "Enter value"
      }
    ],
    "outputs": [
      {
        "id": "output_1758792833170",
        "label": "Voltage",
        "symbol": "V",
        "unit": "Volts (V)",
        "formula": "input_1758792782858 * input_1758792809110",
        "formula_display": "V = I \\cdot R"
      }
    ]
  },
  {
    "id": "air-core-flat-spiral-inductance",
    "title": "Air Core Flat Spiral Inductance Calculator",
    "description": "Calculate the inductance of a flat spiral air core inductor based on dimensions and number of turns.",
    "inputs": [
      {
        "id": "outer_diameter",
        "label": "Outer Diameter",
        "symbol": "d1",
        "unit": "mm",
        "type": "number",
        "required": true,
        "min": 0,
        "placeholder": "Enter outer diameter in mm"
      },
      {
        "id": "inner_diameter",
        "label": "Inner Diameter",
        "symbol": "d2",
        "unit": "mm",
        "type": "number",
        "required": true,
        "min": 0,
        "placeholder": "Enter inner diameter in mm"
      },
      {
        "id": "turns",
        "label": "Number of Turns",
        "symbol": "N",
        "unit": "Turns",
        "type": "number",
        "required": true,
        "min": 1,
        "placeholder": "Enter number of turns"
      },
      {
        "id": "dimension",
        "label": "Dimension Unit Factor",
        "symbol": "k",
        "unit": "",
        "type": "number",
        "required": true,
        "placeholder": "1 for mm, 0.0254 for mils"
      }
    ],
    "outputs": [
      {
        "id": "inductance",
        "label": "Inductance",
        "symbol": "L",
        "unit": "µH",
        "formula": "(4.921 * (turns * turns) * ((outer_diameter + inner_diameter) * (outer_diameter + inner_diameter))) / ((15 * outer_diameter) - (7 * inner_diameter)) * dimension",
        "formula_display": "L = \\frac{4.921 \\cdot N^2 \\cdot (d_1 + d_2)^2}{15d_1 - 7d_2} \\cdot k"
      }
    ],
    "notes": [
      "N = number of turns",
      "d1 = outer diameter (mm)",
      "d2 = inner diameter (mm)",
      "k = dimension factor (1 for mm, 0.0254 for mils)"
    ]
  },
  {
    "id": "symmetric-stripline-impedance-calculator",
    "title": "Symmetric Stripline Impedance Calculator",
    "description": "Calculate characteristic impedance, capacitance, propagation delay, and inductance of a symmetric stripline based on dielectric permittivity, height, trace thickness, and width.",
    "inputs": [
      {
        "id": "substrate_dielectric",
        "label": "Substrate Dielectric",
        "symbol": "ε",
        "unit": "",
        "type": "number",
        "required": true,
        "placeholder": "Enter relative permittivity of dielectric"
      },
      {
        "id": "dielectric_height",
        "label": "Dielectric Height",
        "symbol": "h",
        "unit": "mm",
        "type": "number",
        "required": true,
        "placeholder": "Enter dielectric height"
      },
      {
        "id": "trace_thickness",
        "label": "Trace Thickness",
        "symbol": "t",
        "unit": "mm",
        "type": "number",
        "required": true,
        "placeholder": "Enter trace thickness"
      },
      {
        "id": "trace_width",
        "label": "Width of the Trace",
        "symbol": "w",
        "unit": "mm",
        "type": "number",
        "required": true,
        "placeholder": "Enter width of trace"
      }
    ],
    "outputs": [
      {
        "id": "impedance",
        "label": "Characteristic Impedance",
        "symbol": "Z",
        "unit": "Ohms (Ω)",
        "formula": "60 / Math.sqrt(substrate_dielectric) * Math.log(1.92 * (2 * dielectric_height + trace_thickness) / (0.8 * trace_width + trace_thickness))",
        "formula_display": "Z = \\frac{60}{\\sqrt{\\varepsilon}} \\cdot \\ln\\left(\\frac{1.92 \\cdot (2h + t)}{0.8w + t}\\right)"
      },
      {
        "id": "capacitance",
        "label": "Capacitance per Unit Length",
        "symbol": "C",
        "unit": "F/m",
        "formula": "5.55e-11 * substrate_dielectric / Math.log(3.81 * dielectric_height / (0.8 * trace_width + trace_thickness))",
        "formula_display": "C = \\frac{5.55 \\times 10^{-11} \\cdot \\varepsilon}{\\ln\\left(\\frac{3.81h}{0.8w + t}\\right)}"
      },
      {
        "id": "propagation_delay",
        "label": "Effective Propagation Delay",
        "symbol": "P",
        "unit": "s/m",
        "formula": "3.34e-9 * Math.sqrt(substrate_dielectric)",
        "formula_display": "P = 3.34 \\times 10^{-9} \\cdot \\sqrt{\\varepsilon}"
      },
      {
        "id": "inductance",
        "label": "Inductance per Unit Length",
        "symbol": "L",
        "unit": "H/m",
        "formula": "capacitance * impedance * impedance",
        "formula_display": "L = C \\cdot Z \\cdot Z"
      }
    ]
  },
  {
    "id": "air-core-coil-inductance-calculator",
    "title": "Inductance of An Air Core Coil Calculator",
    "description": "Calculate the inductance of an air core coil based on coil diameter, coil length, and number of turns.",
    "inputs": [
      {
        "id": "coil_diameter",
        "label": "Coil Diameter",
        "symbol": "d",
        "unit": "inches",
        "type": "number",
        "required": true,
        "placeholder": "Enter coil diameter"
      },
      {
        "id": "coil_length",
        "label": "Coil Length",
        "symbol": "l",
        "unit": "inches",
        "type": "number",
        "required": true,
        "placeholder": "Enter coil length"
      },
      {
        "id": "turns",
        "label": "Number of Turns",
        "symbol": "n",
        "unit": "",
        "type": "number",
        "required": true,
        "placeholder": "Enter number of turns"
      }
    ],
    "outputs": [
      {
        "id": "inductance",
        "label": "Inductance",
        "symbol": "L",
        "unit": "µH",
        "formula": "(Math.pow(coil_diameter, 2) * Math.pow(turns, 2)) / (18 * coil_diameter + 40 * coil_length)",
        "formula_display": "L = \\frac{d^2 \\cdot n^2}{18d + 40l}"
      }
    ]
  },
  {
    "id": "pcb-microstrip-crosstalk-calculator",
    "title": "PCB Microstrip Crosstalk Calculator",
    "description": "Calculate the crosstalk (in dB) between two microstrip traces on a PCB based on trace spacing and substrate height.",
    "inputs": [
      {
        "id": "substrate_height",
        "label": "Substrate Height",
        "symbol": "H",
        "unit": "mm",
        "type": "number",
        "required": true,
        "placeholder": "Enter substrate height"
      },
      {
        "id": "trace_spacing",
        "label": "Trace Spacing",
        "symbol": "S",
        "unit": "mm",
        "type": "number",
        "required": true,
        "placeholder": "Enter trace spacing"
      }
    ],
    "outputs": [
      {
        "id": "crosstalk",
        "label": "Crosstalk",
        "symbol": "CTdb",
        "unit": "dB",
        "formula": "20 * Math.log10(1 / (1 + Math.pow(trace_spacing / substrate_height, 2)))",
        "formula_display": "CT_{dB} = 20 \\cdot \\log_{10} \\left( \\frac{1}{1 + (\\frac{S}{H})^2} \\right)"
      }
    ]
  }
]
